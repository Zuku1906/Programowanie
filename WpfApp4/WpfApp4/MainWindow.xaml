        <Label x:Name="label1_Copy1" Content="PESEL" HorizontalAlignment="Left" Margin="522,235,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="radioButton" Content="Knury" HorizontalAlignment="Left" Margin="543,269,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="radioButton_Copy" Content="Lochy" HorizontalAlignment="Left" Margin="647,269,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button3" Content="Wyślij" HorizontalAlignment="Left" Margin="575,297,0,0" VerticalAlignment="Top" Width="109" Height="40" Click="button3_Click"/>

    </Grid>
