        <Button x:Name="button1" Content="Pokaż czas" HorizontalAlignment="Left" Margin="17,123,0,0" VerticalAlignment="Top" Height="74" Width="294" Click="button1_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="625,18,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Margin="626,50,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Kalkulator" HorizontalAlignment="Left" Margin="547,32,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button2" Content="Button" HorizontalAlignment="Left" Margin="627,93,0,0" VerticalAlignment="Top" Width="110" Click="button2_Click"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="577,188,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1_Copy" HorizontalAlignment="Left" Margin="581,213,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1_Copy1" HorizontalAlignment="Left" Margin="576,240,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label1" Content="Imie" HorizontalAlignment="Left" Margin="522,182,0,0" VerticalAlignment="Top"/>
